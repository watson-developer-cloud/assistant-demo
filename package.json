    "@ibm/type": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "ajv": "^6.12.6",
    "autoprefixer": "^10.2.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.3.4",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
